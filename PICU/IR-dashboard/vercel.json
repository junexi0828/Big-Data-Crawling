{
  "routes": [
    {
      "src": "/IR/(.*\\.(html|css|js|png|jpg|jpeg|gif|svg|ico|woff|woff2|ttf|eot|json))",
      "dest": "/IR/$1"
    },
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/(.*\\.(html|css|js|png|jpg|jpeg|gif|svg|ico|woff|woff2|ttf|eot|json))",
      "dest": "/$1"
    }
  ],
  "rewrites": [
    {
      "source": "/",
      "destination": "/IR/index.html"
    },
    {
      "source": "/IR",
      "destination": "/IR/index.html"
    },
    {
      "source": "/IR/",
      "destination": "/IR/index.html"
    },
    {
      "source": "/IR/(.*)",
      "destination": "/IR/$1"
    }
  ]
}
